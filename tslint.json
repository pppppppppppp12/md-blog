{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "only-arrow-functions": false,
        "space-before-function-paren": true,
        "semicolon": [true, "never"],
        "quotemark": [true, "single"],
        "eofline": false,
        "no-trailing-whitespace": false,
        "comment-format": false
    },
    "rulesDirectory": []
}